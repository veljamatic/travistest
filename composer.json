{
    "name": "veljamatic/travistest",
    "description": "A test package for the Laravel and Lumen frameworks.",
    "keywords": [],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Velimir Matic",
            "email": "veljamatic@gmail.com"
        }
    ],
    "require": {
        "php": "^5.5.9 || ^7.0",
        "illuminate/routing": "5.1.* || 5.2.* || 5.3.*",
        "illuminate/support": "5.1.* || 5.2.* || 5.3.*"
    },
    "require-dev": {
        "laravel/lumen-framework": "5.1.* || 5.2.*",
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "^4.8 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Veljamatic\\Travistest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Veljamatic\\Travistest\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
